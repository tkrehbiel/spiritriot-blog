{
    "plugins": ["@typescript-eslint", "jest"],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "next/core-web-vitals",
      "prettier"
    ],
    "env": {
      "jest/globals": true
    },
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "off"
    }
}
